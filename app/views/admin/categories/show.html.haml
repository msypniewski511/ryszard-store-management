%h3= @admin_category.name
%table.show
  %tr
    %th Name
    %th Description
  %tr
    %td= @admin_category.name
    %td= @admin_category.description
  %tr
    %td{colspan: 2}
      = link_to 'Back', admin_categories_path, remote: true |
      = link_to 'Edit', edit_admin_category_path(@admin_category), remote: true