:javascript
  $(document).ready(function(){
  	$("tr[data-link]").click(function() {
  	  var cos = $(this).attr("data-link");
  	  alert(cos)
      window.location = cos
    });
  });
-if notice
  %p#notice
    = "#{notice}"
#product_list
  = render partial: 'search_box'
  - if @products
    %p
      = "Wyszukiwanie dla: \"" + params[:query] + "\" dalo wynik:"
    %hr
    %table#product_list
      %tbody
        = render partial: 'shared/products', collection: @products
        %tr= link_to 'Catalog', catalog_index_path